{
    "python.pythonPath": "python",  
    "python.venvPath": "~/venvs/",

    "python.jediPath": "~/venvs/jedi/lib/python3.10/site-packages/",  
    "python.jediEnabled": true,  
    "suggest.timeout": 5000,

    "python.formatting.provider": "black",  
    "python.formatting.blackPath": "~/venvs/black/bin/black",

    "python.linting.pylintEnabled": true,  
    "python.linting.flake8Enabled": false,  
    "python.linting.pylintPath": "~/venvs/pylint/bin/pylint",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "python",
    "json"
  ],
  "python.linting.flake8Enabled": false,
  "python.linting.enabled":true,
  "python.linting.pylintEnabled":true,
  "python.formatting.provider": "black",

  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "coc.preferences.diagnostic.virtualText": true
}

